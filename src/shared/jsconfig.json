{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "rootDir": "./",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "strict": true,
    "skipLibCheck": true
  }, 
  "exclude": ["node_modules"],
  "include": ["types/**/*", "utils/**/*"],
}